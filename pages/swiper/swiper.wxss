
/*轮播图样式*/
swiper-container{  
  position: relative;  
}  
.swiper-container .swiper{  
  height: 158px;  
}  
.swiper-container .swiper .img{  
  width: 100%;  
  height: 100%;  
}  
.swiper-container .dots{  
  position: relative;  
  left: 0;  
  right: 0;  
  bottom: 40rpx;  
  display: flex;  
  justify-content: center;  
}  
.swiper-container .dots .dot{  
  margin: 0 8rpx;  
  width: 20rpx;  
  height: 20rpx;  
  border:1px solid #ffffff;  
  border-radius: 50%;  
  transition: all .6s;  
}  
.swiper-container .dots .dot.active{  
  width: 20rpx;  
  height: 20rpx;  
  background: #ffffff;  
}  
.swiper video {

  height: 100%;

  width: 100%;

}
.num{
   position: relative;  
   color:white;
  width: 50px;
  float:right;
  text-align: center;
  justify-content: center;  
  bottom:50rpx;
  background:#cccccc;
  /* border-bottom-left-radius:10px;
  border-top-left-radius: 10px; */
  border-radius: 10px;
}



/**index.wxss**/
.swiper-item{position: relative; width: 100%;}
.swiper_banner swiper-item image{width: 100%; height: 400rpx;}
.swiper-box .wx-swiper-dots.wx-swiper-dots-horizontal{width: 100%; padding-right: 30rpx;display: flex;justify-content:flex-end;box-sizing: border-box;}
.nav{ width: 100%; padding:0 10rpx; box-sizing: border-box; height: 197rpx; align-items: center; background-color: #fff;}
