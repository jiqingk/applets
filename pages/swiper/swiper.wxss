/*BILLING*/
     .wrap{height:47px;overflow: hidden;position: absolute;top:30;left: 100;width: 100%}
      .billing{margin:0;height: 47px;width: 100%}
      .content .billing{
        position: absolute;
      }
      @-webkit-keyframes anim1{
        0% {top: 47px;opacity: 1}
        50% {top: -47px;opacity: 1}
        75% {top: -47px ;opacity: 0}
        100%{top:47px;opacity: 0}
      }
      @-webkit-keyframes anim2{
     
        0% {top: -47px;opacity: 0}
        25% {top: 47px;opacity: 0}
        50% {top: 47px;opacity: 1}
        100%{top: -47px;opacity: 1}
      }

      .content .billing:nth-child(1){background-color: red;}
      .content .billing:nth-child(2){background-color: yellow;}
      .content .billing:nth-child(1){
        
        -webkit-animation: anim1 5s linear infinite;
      }
      
      .content .billing:nth-child(2){
    
        -webkit-animation: anim2 5s linear infinite;
      }